<%= form_with model: journal, url: single_entries_path, class:'entry__form-group', remote: true do |f| %>
  <div class='entry__index-tr'>
    <div class='entry__new-td--date'>
      <%= f.text_field :month, autofocus: true, tabindex: 11, class:'entry__form--date' %>
    </div>
    <div class='entry__new-td--date'>
      <%= f.text_field :day, tabindex: 12, class:'entry__form--date' %>
    </div>
    <div class='entry__new-td--code'>
      <%= f.text_field :debit_code, tabindex: 13, class:'entry__form--code' %>
    </div>
    <div class='entry__new-td--name'>
      <%= f.text_field :debit_name, disabled: true, class:'entry__form--name' %>
    </div>
    <div class='entry__new-td--code'>
      <%= f.text_field :credit_code, tabindex: 14, class:'entry__form--code' %>
    </div>
    <div class='entry__new-td--name'>
      <%= f.text_field :credit_name, disabled: true, class:'entry__form--name' %>
    </div>
    <div class='entry__new-td--amount'>
      <%= f.text_field :amount, tabindex: 15, class:'entry__form--amount' %>
    </div>
    <div class='entry__new-td--description'>
      <%= f.text_field :description, tabindex: 16, class:'entry__form--description' %>
    </div>
    <div class='entry__new-td--btn'>
      <%= f.submit '', tabindex: 17 %>
    </div>
  </div>
<% end %>

<!-- 科目候補表示 -->
<div class='entry__subbox'></div>