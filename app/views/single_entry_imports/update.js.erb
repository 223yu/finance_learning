<% if flash[:danger] %>
  $('.flash').html('<%= j(render 'layouts/flash') %>');
  $('.entry__index-tr<%= @import.id %>').html('<%= j(render 'single_entry_imports/edit', import: @import) %>');
<% else %>
  $('.flash').html('<%= j(render 'layouts/flash') %>');
  $('.entry__index-tr<%= @import.id %>').html('<%= j(render 'single_entry_imports/show', import: @import) %>');
<% end %>


